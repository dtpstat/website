{
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint --report-unused-disable-directives \"**/*\"",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "start": "next start",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-testing-library": "^5.0.0"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "dayjs": "^1.10.7",
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.45.1",
    "scss": "^0.2.4",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "^0.2.3",
    "@kachkaev/markdownlint-config": "^0.2.0",
    "@netlify/plugin-nextjs": "^4.0.0-rc.2",
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/styled-components": "^5.1.18",
    "eslint": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "markdownlint-cli": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "prettier-plugin-sh": "^0.8.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0",

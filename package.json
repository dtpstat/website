{
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint --report-unused-disable-directives \"**/*\"",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "start": "next start",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "suppress-exit-code eslint --fix",
      "suppress-exit-code markdownlint --fix",
      "suppress-exit-code prettier --write"
    ]
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.7.0",
    "@prisma/client": "^3.8.1",
    "dayjs": "^1.10.7",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gravatar": "^2.6.3",
    "react-linkify": "^1.0.0-alpha",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "^0.4.5",
    "@kachkaev/markdownlint-config": "^0.3.0",
    "@netlify/plugin-nextjs": "^4.2.0",
    "@next/eslint-plugin-next": "^12.0.8",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-gravatar": "^2.6.10",
    "@types/react-linkify": "^1.0.1",
    "@types/styled-components": "^5.1.21",
    "eslint": "^8.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "markdownlint-cli": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "prettier-plugin-sh": "^0.8.1",
    "prisma": "^3.8.1",
    "suppress-exit-code": "^1.0.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0",
    "yarn": "^1.22.0"
  },
  "packageManager": "yarn@3.1.1"
}

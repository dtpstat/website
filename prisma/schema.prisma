generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model comments {
  id          Int       @id @default(autoincrement())
  userId     Int?
  text        String?
  createDate DateTime? @db.Date
}

model users {
  id         Int     @id @default(autoincrement())
  name       String?
  email      String?
  avatarUrl String?
}

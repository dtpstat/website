generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model comments {
  id           Int       @id @default(autoincrement())
  text         String?
  createDate   DateTime? @db.Date
  auth0userSub String? @db.VarChar
}

model users {
  id           Int     @id @default(autoincrement())
  name         String?
  email        String?
  avatarUrl    String?
  auth0userSub String? @db.VarChar
}
